{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      experimentalObjectRestSpread: true
    }
  },
  "globals": {
    "__DEVELOPMENT__": true,
    "__BASENAME__": ""
  },
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "ignore"
    }],
    "no-bitwise": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
  },
  "plugins": [
    "import"
  ]
}
